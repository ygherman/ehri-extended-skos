 xmlns:dc="http://purl.org/dc/terms/"
>
<!-- The URI of the skos extension is not final. which URI can be used here? -->
<owl:Ontology> rdf:about=""http://ehri01.dans.knaw.nl/terms/ehri-skos-ext#"
<dc:title>"EHRI SKOS Extension</dc:title>
<dc:description >"Extention of SKOS scheme to adjust the rdfs:label property skos:preflabel to the EHRI Thesaurus terms in the languages, in which the term have more than one preferred label (singular), in each gender."</dc:description>
<dc:issued>"2013-10-20</dc:issued> 
<dc:modified>"2013-10-20"</dc:modified>
</owl:Ontology>


<!-- Definition of ehri:prefMaleLabel -->
<rdf:Property rdf:about="http://ehri01.dans.knaw.nl/terms/ehri-skos-ext#prefMaleLabel">
<rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"></rdfs:isDefinedBy>
<rdfs:label xml:lang="en">preferred Male Label </rdfs:label>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
<rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"></rdfs:subPropertyOf>
<skos:difinition xml:lang="en">The preferred singular Male label for a term, in the languages which thesaurus terms are in singular, and a different term is used for singular male"</skos:difinition>
<rdfs:comment xml:lang="en">No two concepts in the same concept scheme may have the same value for skos:prefMaleLabel.</rdfs:comment>
<rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"/>
<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
</rdf:Property>


<!-- Definition of ehri:prefFemaleLabel -->
<rdf:Property rdf:about="http://ehri01.dans.knaw.nl/terms/ehri-skos-ext#prefFemaleLabel">
<rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"></rdfs:isDefinedBy>
<rdfs:label xml:lang="en">preferred Male Label </rdfs:label>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
<rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"></rdfs:subPropertyOf>
<skos:difinition xml:lang="en">The preferred singular feminine label for a term, in the languages which thesaurus terms are in singular, and a different term is used for singular female"</skos:difinition>
<rdfs:comment xml:lang="en">No two concepts in the same concept scheme may have the same value for skos:prefFemaleLabel.</rdfs:comment>
<rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"/>
<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
</rdf:Property>

